 
 <div class="d-view">
 <a href="/"><%= image_tag 'SNOWAIDiconyoko.png', size: '141x22' ,class: "d-logo"  %></a>
 <br>
 <a class="d-title">-ログイン-</a>
 
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :メールアドレス %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" ,:class=>"n-btn" %>
  </div>

  <div class="field">
    <%= f.label :パスワード %><br />
    <%= f.password_field :password, autocomplete: "current-password" ,:class=>"n-btn" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :パスワードを記憶する %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in" ,:class=>"s-btn" %>
  </div>
<% end %>
<br>
<%= render "devise/shared/links" %>
</div>